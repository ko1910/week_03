#include <bits/stdc++.h>

using namespace std;

int main(){
    int q;
    cin >> q;
    string h="hackerrank";
    for(int a0 = 0; a0 < q; a0++){
        string s;
        cin >> s;
        int index=0;
        for(int i=0; i<(int)s.size(); i++){
            if(s[i]==h[index]){
                index++;
            }
        }
        if(index==h.size()){
            cout<<"YES"<<endl;
        }else{
            cout<<"NO"<<endl;
        }
    }
    return 0;
}
